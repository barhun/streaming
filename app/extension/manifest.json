{
  "background": {
    "persistent": false,
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["content.js"],
      "matches": ["<all_urls>"],
      "run_at": "document_start"
    }
  ],
  "manifest_version": 2,
  "name": "extension",
  "permissions": [
    "privacy"
  ],
  "version": "1.0.0"
}
